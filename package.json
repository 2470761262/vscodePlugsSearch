{
	"name": "inlineSearch",
	"displayName": "inlineSearch",
	"description": "inlineSeacrchBaidu",
	"publisher": "zrrz",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.50.0"
	},
	"keywords": [
		"baidu",
		"query",
		"select",
		"鑫家网"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:inlineSearch.text"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "inlineSearch.text",
				"title": "百度查询"
			}
		],
		"keybindings": [
			{
					"command": "inlineSearch.text",
					"key": "alt+f2",
					"mac": "cmd+f2",
					"when": "editorFocus"
			}
	]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {}
}
